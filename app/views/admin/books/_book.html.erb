<tr>
  <td>
    <%= image_tag "user/avatar.png", alt: "user", class: "img-radius img-fluid wid-60" %>
  </td>
  <td>
    <%= book.name %>
  </td>
  <td class="w-400">
    <%= book.description %>
  </td>
  <td>
    <%= book.quantity %>
  </td>
  <td>
    <%= book.author.name %>
  </td>
  <td>
    <%= book.publiser.name %>
  </td>
  <td class="w-180">
    <%= link_to "javascript:void(0)",
      {class: "btn btn-icon btn-success show_user",
        data: { bs_toggle: "modal",
        bs_target: "#exampleModalCenter"}} do %>
      <i class="icon feather icon-eye f-16"></i>
    <% end %>
    <%= link_to "javascript:void(0)",
      {class: "btn btn-icon btn-warning active_user",
        data: { url: "#",
        bs_placement: "top"}} do %>
      <i class="icon feather icon-check-square f-16"></i>
    <% end %>
  </td>
</tr>
